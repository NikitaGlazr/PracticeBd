<Window x:Class="Practice.Pages.AddOrderItem"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practice.Pages"
        mc:Ignorable="d"
        Title="AddOrderItem" Height="450" Width="800">
    <Grid Margin="0,0,0,-6">
        <Label Content="Название продукта" HorizontalAlignment="Left" Margin="30,26,0,0" VerticalAlignment="Top"/>
        <Label Content="Количество" HorizontalAlignment="Left" Margin="30,137,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="ProductNameTextBox" IsReadOnly="True" Margin="30,87,400,326"/>
        <TextBox x:Name="QuantityTextBox" IsReadOnly="True" Margin="30,196,400,217"/>
        <Button Content="Добавить" HorizontalAlignment="Left" Margin="30,272,0,0" VerticalAlignment="Top" Click="ButtonAddOrderItem_Click"/>
        <Button Content="Очистить" HorizontalAlignment="Left" Margin="274,272,0,0" VerticalAlignment="Top" Click="ButtonClear_Click"/>

    </Grid>
</Window>
